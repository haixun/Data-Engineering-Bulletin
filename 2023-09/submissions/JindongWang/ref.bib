  booktitle = {Proceedings of the 25th {{International Conference}} on {{World Wide Web}}},
  author = {Pellissier Tanon, Thomas and Vrande{\v c}i{\'c}, Denny and Schaffert, Sebastian and Steiner, Thomas and Pintscher, Lydia},
  year = {2016},
