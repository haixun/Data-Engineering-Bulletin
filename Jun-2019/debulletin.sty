	\newcommand{\opinionauthor}{#2}
	\newcommand{\opinionaddress}{#3}
	\tocsubentry{\opiniontitle}{\opinionauthor}
	\heading{\opiniontitle}
        {\opinionauthor}\\{\opinionaddress}
	\makeatletter
        \@RestoreMaketitle
	\@UndefinePreambleCommands
